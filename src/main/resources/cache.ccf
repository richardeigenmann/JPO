# DEFAULT CACHE REGION
jcs.default=DC
jcs.default.cacheattributes=org.apache.commons.jcs3.engine.CompositeCacheAttributes
jcs.default.cacheattributes.MaxObjects=5000
jcs.default.cacheattributes.MemoryCacheName=org.apache.commons.jcs3.engine.memory.lru.LRUMemoryCache
jcs.default.cacheattributes.UseMemoryShrinker=true
jcs.default.cacheattributes.MaxMemoryIdleTimeSeconds=3600
jcs.default.cacheattributes.ShrinkerIntervalSeconds=60
jcs.default.elementattributes=org.apache.commons.jcs3.engine.ElementAttributes
jcs.default.elementattributes.IsEternal=false
jcs.default.elementattributes.MaxLifeSeconds=21600
jcs.default.elementattributes.IdleTime=1800
jcs.default.elementattributes.IsSpool=true
jcs.default.elementattributes.IsRemote=true
jcs.default.elementattributes.IsLateral=true


# PREDEFINED CACHE REGIONS for Highres no disk cache (Images came from disk)
jcs.region.highresCache=
jcs.region.highresCache.cacheattributes=org.apache.commons.jcs3.engine.CompositeCacheAttributes
# Max highres pictures in cache: 100
#    5 @ 1000M OK
#   50 @ 1000M Blows out of memory errors when loading large pictures
#  100 @ 1000M Blows out of memory errors when loading large pictures
# 1000 @ 1000M Blows out of memory errors
jcs.region.highresCache.cacheattributes.MaxObjects=100
#jcs.region.highresCache.cacheattributes.MemoryCacheName=
    org.apache.commons.jcs3.engine.memory.lru.LRUMemoryCache
#jcs.region.highresCache.cacheattributes.MemoryCacheName=
    org.apache.commons.jcs3.engine.memory.lru.LHMLRUMemoryCache
#jcs.region.highresCache.cacheattributes.MemoryCacheName=
    org.apache.commons.jcs3.engine.memory.fifo.FIFOMemoryCache
jcs.region.highresCache.cacheattributes.MemoryCacheName=
    org.apache.commons.jcs3.engine.memory.mru.MRUMemoryCache
jcs.region.highresCache.cacheattributes.UseMemoryShrinker=true
jcs.region.highresCache.cacheattributes.MaxMemoryIdleTimeSeconds=3600
jcs.region.highresCache.cacheattributes.ShrinkerIntervalSeconds=60
jcs.region.highresCache.cacheattributes.MaxSpoolPerRun=500
jcs.region.highresCache.elementattributes=
    org.apache.commons.jcs3.engine.ElementAttributes
jcs.region.highresCache.elementattributes.IsEternal=false


# PREDEFINED CACHE REGIONS
jcs.region.thumbnailCache=DC
jcs.region.thumbnailCache.cacheattributes=org.apache.commons.jcs3.engine.CompositeCacheAttributes
# 200 @ 1000M 
# 500 @ 1000M Out of Memory
jcs.region.thumbnailCache.cacheattributes.MaxObjects=500
jcs.region.thumbnailCache.cacheattributes.MemoryCacheName=
    org.apache.commons.jcs3.engine.memory.lru.LRUMemoryCache
jcs.region.thumbnailCache.cacheattributes.UseMemoryShrinker=true
jcs.region.thumbnailCache.cacheattributes.MaxMemoryIdleTimeSeconds=3600
jcs.region.thumbnailCache.cacheattributes.ShrinkerIntervalSeconds=60
jcs.region.thumbnailCache.cacheattributes.MaxSpoolPerRun=500
jcs.region.thumbnailCache.elementattributes=org.apache.commons.jcs3.engine.ElementAttributes
jcs.region.thumbnailCache.elementattributes.IsEternal=false
jcs.region.thumbnailCache.cacheattributes.DiskUsagePatternName=UPDATE

# AVAILABLE AUXILIARY CACHES
jcs.auxiliary.DC=
    org.apache.commons.jcs3.auxiliary.disk.indexed.IndexedDiskCacheFactory
jcs.auxiliary.DC.attributes=
    org.apache.commons.jcs3.auxiliary.disk.indexed.IndexedDiskCacheAttributes
jcs.auxiliary.DC.attributes.DiskPath=/tmp/Jpo-JCS-cache
jcs.auxiliary.DC.attributes.MaxPurgatorySize=5000  
jcs.auxiliary.DC.attributes.MaxKeySize=1000000
jcs.auxiliary.DC.attributes.MaxRecycleBinSize=5000
jcs.auxiliary.DC.attributes.OptimizeAtRemoveCount=300000
jcs.auxiliary.DC.attributes.ShutdownSpoolTimeLimit=60
jcs.auxiliary.DC.attributes.OptimizeOnShutdown=true
